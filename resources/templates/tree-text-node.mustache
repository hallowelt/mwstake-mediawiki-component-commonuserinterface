<li{{#id}} id="{{.}}"{{/id}} class="mws-tree-node{{class}}" {{#role}} role="{{.}}"{{/role}}>
	<div>
		{{#expandBtn}}<a class="mws-tree-expander{{class}}" aria-expanded="{{expanded}}" role="button" tabindex="0" aria-label="{{text}}"{{#id}} aria-controlls="{{.}}-children"{{/id}}{{#hasChildren}} aria-haspopup="true"{{/hasChildren}}></a>{{/expandBtn}}<span id="{{labelId}}" class="mws-tree-node-label{{labelclass}}" tabindex="0">{{text}}</span>
	</div>
	{{#hasChildren}}
	<ul aria-labelledby="{{labelId}}" class="mws-tree-node-children{{#expanded}} show{{/expanded}}"{{#id}} id="{{.}}-children"{{/id}}>
		{{#children}}{{{.}}}{{/children}}
	</ul>
	{{/hasChildren}}
</li>