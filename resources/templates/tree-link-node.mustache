<li{{#id}} id="{{.}}"{{/id}} {{#class}} class="{{.}}"{{/class}} {{#role}} role="{{.}}"{{/role}}>
	<div id="{{labelId}}">
		<a href="{{href}} title="{{title}}">{{text}}</a>
	</div>
	{{#hasChildren}}
	<ul role="tree" aria-labelledby="{{labelId}}" class="tree-node-children">
		{{#children}}{{{.}}}{{/children}}
	</ul>
	{{/hasChildren}}
</li>