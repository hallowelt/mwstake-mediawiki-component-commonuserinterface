<li{{#id}} id="{{.}}"{{/id}} class="mws-tree-item {{class}}" {{#role}} role="{{.}}"{{/role}}>
	<div>
		{{#expandBtn}}<a class="mws-tree-expander {{class}}" role="button" tabindex="0" aria-label="{{text}}"{{#id}} aria-controls="{{.}}-children"{{/id}}{{#hasChildren}} aria-expanded="{{expanded}}" aria-haspopup="true"{{/hasChildren}}></a>{{/expandBtn}}<a id="{{labelId}}" class="mws-tree-item-label{{#isNew}} new{{/isNew}}" href="{{href}}"{{#title}} title="{{{.}}}"{{/title}}{{#rel}} rel="{{{.}}}"{{/rel}}{{#target}} target="{{{.}}}"{{/target}}>{{{text}}}</a>
	</div>
	{{#hasChildren}}
	<ul class="mws-tree mws-tree-item-children{{#expanded}} show{{/expanded}}"{{#id}} id="{{.}}-children"{{/id}} role="tree" aria-labelledby="{{labelId}}" tabindex="0">
		{{#children}}{{{.}}}{{/children}}
	</ul>
	{{/hasChildren}}
</li>
