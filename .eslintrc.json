{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "commonjs"
	},
	"globals": {
		"jQuery": "readonly",
		"mediaWiki": "readonly"
	},
	"rules": {
		"no-jquery/no-class-state": "off",
		"no-jquery/variable-pattern": "off"
	}
}
